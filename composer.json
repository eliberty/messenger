{
    "name": "eliberty/symfony-messenger",
    "type": "library",
    "description": "Symfony Messenger Component",
    "keywords": [],
    "homepage": "https://symfony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Roze",
            "email": "samuel.roze@gmail.com"
        },
        {
            "name": "Symfony Community",
            "homepage": "https://symfony.com/contributors"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "psr/log": "~1.0",
        "eliberty/symfony-amqp-messenger": "3.4.x-dev",
        "symfony/deprecation-contracts": "^2.1",
        "eliberty/symfony-doctrine-messenger": "3.4.x-dev",
        "symfony/polyfill-php80": "^1.15",
        "eliberty/symfony-redis-messenger": "3.4.x-dev",
        "eliberty/amazon-sqs-messenger": "3.4.1"
    },
    "require-dev": {
        "psr/cache": "~1.0",
        "symfony/console": "~3.4|^4.4|^5.0",
        "symfony/dependency-injection": "~3.4|^4.4|^5.0",
        "symfony/event-dispatcher": "~3.4|^4.4|^5.0",
        "symfony/http-kernel": "~3.4|^4.4|^5.0",
        "symfony/process": "~3.4|^4.4|^5.0",
        "symfony/property-access": "~3.4|^4.4|^5.0",
        "symfony/serializer": "~3.4|^4.4|^5.0",
        "symfony/service-contracts": "^1.1|^2",
        "symfony/stopwatch": "~3.4|^4.4|^5.0",
        "symfony/validator": "~3.4|^4.4|^5.0"
    },
    "conflict": {
        "symfony/event-dispatcher": "<3.4",
        "symfony/http-kernel": "<3.4"
    },
    "suggest": {
        "enqueue/messenger-adapter": "For using the php-enqueue library as a transport."
    },
    "autoload": {
        "psr-4": { "Symfony\\Component\\Messenger\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-3.4": "3.4.x-dev"
        }
    }
}
